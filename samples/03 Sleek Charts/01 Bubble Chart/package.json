{
  "name": "bubblechart",
  "version": "1.0.0",
  "description": "A Bubble Chart implemented with D3.js and making use of ES6 modules plus Webpack 2 bundling features",
  "main": "dist/app.js",
  "scripts": {
    "start": "webpack-dev-server --config=dev.webpack.config.js",
    "build:dev": "rimraf dist && webpack --config=dev.webpack.config.js",
    "build:prod": "rimraf dist && webpack -p --config=prod.webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Lemoncode/d3js-samples.git"
  },
  "keywords": [
    "d3",
    "js",
    "graphs",
    "javascript",
    "modules"
  ],
  "author": "Javier Calzado",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Lemoncode/d3js-samples/issues"
  },
  "homepage": "https://github.com/Lemoncode/d3js-samples#readme",
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "babel-preset-env": "^1.1.11",
    "babel-preset-react": "^6.23.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.2",
    "extract-text-webpack-plugin": "^2.0.0",
    "file-loader": "^0.10.1",
    "html-webpack-plugin": "^2.28.0",
    "rimraf": "^2.6.1",
    "style-loader": "^0.13.2",
    "url-loader": "^0.5.8",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1",
    "webpack-merge": "^4.0.0"
  },
  "dependencies": {
    "d3": "^4.7.4"
  }
}
